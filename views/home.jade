extends layout

block content
  header#myCarousel.carousel.slide.jumbotronHome
    //- Indicators
    ol.carousel-indicators
      li.active(data-target='#myCarousel', data-slide-to='0')
      li(data-target='#myCarousel', data-slide-to='1')
      li(data-target='#myCarousel', data-slide-to='2')
    //- Wrapper for Slides
    .carousel-inner
      .item.active
        //- Set the first background image using inline CSS below.
        .fill(style="background-image:url('img/b5a368be53ad4b20bb3441cb26f2fc44.jpg');")
        .carousel-caption
          h2 Loffo
      .item
        //- Set the second background image using inline CSS below.
        .fill(style="background-image:url('img/a_banner-01.jpg');")
        .carousel-caption
          h2 Caption 2
      .item
        //- Set the third background image using inline CSS below.
        .fill(style="background-image:url('img/b_banner.jpg');")
        .carousel-caption
          h2 Caption 3
    //- Controls
    a.left.carousel-control(href='#myCarousel', data-slide='prev')
      span.icon-prev
    a.right.carousel-control(href='#myCarousel', data-slide='next')
      span.icon-next
